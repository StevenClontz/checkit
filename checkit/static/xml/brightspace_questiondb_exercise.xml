              − --&gt;&lt;/mo&gt;&lt;mn&gt;15&lt;/mn&gt;&lt;mspace
              width="thinmathspace" /&gt;&lt;mrow
              class="MJX-TeXAtom-ORD"&gt;&lt;mi&gt;y&lt;/mi&gt;&lt;/mrow&gt;&lt;mo&gt;=&lt;/mo&gt;&lt;mn&gt;0&lt;/mn&gt;&lt;mo&gt;,&lt;/mo&gt;&lt;mspace
              width="1em" /&gt;&lt;mi&gt;y&lt;/mi&gt;&lt;mrow
              class="MJX-TeXAtom-ORD"&gt;&lt;mo maxsize="1.2em"
              minsize="1.2em"&gt;(&lt;/mo&gt;&lt;/mrow&gt;&lt;mi&gt;log&lt;/mi&gt;&lt;mo&gt;&amp;#x2061;&lt;!--
              ⁡
              --&gt;&lt;/mo&gt;&lt;mrow&gt;&lt;mo&gt;(&lt;/mo&gt;&lt;mn&gt;3&lt;/mn&gt;&lt;mo&gt;)&lt;/mo&gt;&lt;/mrow&gt;&lt;mrow
              class="MJX-TeXAtom-ORD"&gt;&lt;mo maxsize="1.2em"
              minsize="1.2em"&gt;)&lt;/mo&gt;&lt;/mrow&gt;&lt;mo&gt;=&lt;/mo&gt;&lt;mo&gt;&amp;#x2212;&lt;!--
              −
              --&gt;&lt;/mo&gt;&lt;mn&gt;54&lt;/mn&gt;&lt;/mstyle&gt;&lt;annotation
              encoding="latex"&gt;{"version":"1.1","math":"5 \, {y'} - 15 \,
              {y} = 0,\hspace{1em}
              y\big(\log\left(3\right)\big)=-54"}&lt;/annotation&gt;&lt;/semantics&gt;&lt;/math&gt;&lt;/p&gt;</mattext>
            </material>
